      const userCardTemplate=document.querySelector("[data-user-template]")
      const userCardContainer=documnet.querySelector("[data-user-cards-container]")
      const searchInput=document.querySelector("[data-search]")
      let users[];
      searchInput.addEventListner("input",(e)=>{
        const value=e.target.value.toLowerCase()
        users.forEach(user=>{
          const isVisible=
          user.name.toLowerCase().includes(value)||
          user.email.toLowerCase().includes(value)
          user.element.classList.toggle("hide",!isVisible)
        })
      })
      fetch("https://jsonplaceholder.typicode.com/users")
      .then(res=>res.json())
      .then(data=>{
        user=data.map(user=>{
          const card=userCardTemplate.content.cloneNode(true).children[0]
          const header=card-body.querySelector("[data-card-title]")
          const body=card.querySelector("[data-card-text]")
          const item=card.querySelector("[data-list-group-item]")
          header.textContent=user.name
          body.textContent=user.email
          return {name:user.name,email:user.email,element:card-body}
        })
      })
